<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>  

<VBox 
    styleClass="background" 
    alignment="CENTER" 
    prefHeight="400" prefWidth="340" spacing="20" 
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="controller.SeasonController"
>
    
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>

    <ImageView fitWidth="340" fitHeight="210" preserveRatio="false">
        <Image url="@/view/NBA_Back.jpg"/>
    </ImageView>
    <VBox alignment="CENTER" spacing='10'>
        <VBox alignment="CENTER" spacing='30'>
            <Button text="Round" styleClass="menu-button" onAction="#seeRound"/>
            <Button text="Current" styleClass="menu-button" onAction="#seeCurrent"/>
            <Button text="Game" styleClass="menu-button" onAction="#seeGame"/>
            <Button text="Result" styleClass="menu-button" onAction="#seeResults"/>
            <Button text="Close" styleClass="menu-button" onAction="#close"/>
        </VBox>
     </VBox>
    <Region prefHeight="00" />
</VBox>
