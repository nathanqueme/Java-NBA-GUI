<VBox 
    alignment="CENTER" 
    xmlns:fx="http://javafx.com/fxml/1"  
    styleClass="background"
    fx:controller="controller.TeamsController"
>
   
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>
    
  
    
    <HBox alignment="CENTER" spacing="10">
        <Button text="Add" onAction="#handleAddTeam"/>
        <Button text="Manage" onAction="#handleManageTeam"/>
        <Button text="Delete" onAction="#handleDeleteTeam"/>
        <Button text="Close" onAction="#close"/>
    </HBox>
</VBox>

